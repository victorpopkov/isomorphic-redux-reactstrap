{
  "name": "universal-redux",
  "version": "0.1.0",
  "description": "Universal React and Redux web app boilerplate bundled with webpack.",
  "author": {
    "name": "Victor Popkov",
    "email": "victor@popkov.com"
  },
  "repository": {
    "type": "git",
    "url": "github:victorpopkov/universal-redux"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/victorpopkov/universal-redux/blob/master/LICENSE"
    }
  ],
  "main": "bin/server.js",
  "scripts": {
    "build": "npm-run-all build:client build:server",
    "build:client": "better-npm-run build:client",
    "build:server": "better-npm-run build:server",
    "dev": "npm-run-all universal-webpack:prepare development",
    "development": "npm-run-all --parallel watch:client watch:server start:dev start:dev:api",
    "lint": "eslint -c .eslintrc src api; remark .",
    "start": "npm-run-all --parallel start:prod start:prod:api",
    "start:dev": "node ./bin/server.js",
    "start:dev:api": "node ./bin/api.js",
    "start:prod": "node ./bin/server.js",
    "start:prod:api": "node ./bin/api.js",
    "universal-webpack:prepare": "universal-webpack --settings ./webpack/universal-webpack-settings.json prepare",
    "watch:client": "better-npm-run watch:client",
    "watch:server": "better-npm-run watch:server"
  },
  "betterScripts": {
    "build:client": {
      "command": "webpack --config ./webpack/webpack.config.client.prod.babel.js --colors --display-error-details --verbose"
    },
    "build:server": {
      "command": "webpack --config ./webpack/webpack.config.server.prod.babel.js --colors --display-error-details --verbose"
    },
    "watch:client": {
      "command": "node ./webpack/webpack-dev-server.js"
    },
    "watch:server": {
      "command": "webpack --config ./webpack/webpack.config.server.dev.babel.js --colors --display-error-details --watch"
    }
  },

    "compression": "^1.7.2",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "history": "^4.7.2",
    "http-proxy": "^1.17.0",
    "humps": "^2.0.1",
    "immutable": "^3.8.2",
    "normalizr": "^3.2.4",

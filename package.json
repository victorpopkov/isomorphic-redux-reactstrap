    "react": "^16.3.2",
    "react-cookie": "^2.1.4",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.3.2",
    "react-progress-2": "^4.4.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-bootstrap": "^0.24.4",

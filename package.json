{
  "name": "isomorphic-redux-reactstrap",
  "description": "Isomorphic web app boilerplate built using React, Redux and Reactstrap.",
  "author": "Victor Popkov <victor@popkov.com>",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "github:victorpopkov/isomorphic-redux-reactstrap"
  },
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrently --kill-others \"npm run start:prod\" \"npm run start:prod:api\"",
    "start:prod": "better-npm-run start:prod",
    "start:prod:api": "better-npm-run start:prod:api",
    "build": "better-npm-run build",
    "lint": "eslint -c .eslintrc src api; remark .",
    "start:dev": "better-npm-run start:dev",
    "start:dev:api": "better-npm-run start:dev:api",
    "watch:client": "better-npm-run watch:client",
    "dev": "concurrently --kill-others \"npm run watch:client\" \"npm run start:dev\" \"npm run start:dev:api\""
  },
  "betterScripts": {
    "start:prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "BABEL_ENV": "production",
        "PORT": 8080,
        "API_SCHEMA": "http",
        "API_HOST": "localhost",
        "API_PORT": 3030,
        "API_PREFIX": ""
      }
    },
    "start:prod:api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "API_PORT": 3030
      }
    },
    "start:dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "BABEL_ENV": "development",
        "PORT": 3000,
        "API_SCHEMA": "http",
        "API_HOST": "localhost",
        "API_PORT": 3030,
        "API_PREFIX": ""
      }
    },
    "start:dev:api": {
      "command": "node ./bin/api.js",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "API_PORT": 3030
      }
    },
    "watch:client": {
      "command": "node ./webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "BABEL_ENV": "development",
        "PORT": 3000,
        "API_PORT": 80
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "BABEL_ENV": "production"
      }
    }
  },
